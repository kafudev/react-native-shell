  "name": "rnshell",
  "version": "0.64.1.1",
  "private": true,
  "scripts": {
    "start": "yarn react-native start",
    "android": "yarn react-native run-android",
    "ios": "yarn react-native run-ios --simulator 'iPhone Xr'",
    "ipad": "yarn react-native run-ios --simulator 'iPad Air 2'",
    "build-ios": "yarn react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/index.jsbundle --sourcemap-output ios/index.jsbundle.map --assets-dest ios/",
    "build-android": "cd android && ./gradlew clean app:assembleRelease --profile && cd ../",
    "install-android": "cd android && ./gradlew clean app:installRelease --info --profile && cd ../",
    "push-ios": "appcenter codepush release-react -a zpd106/ruchen_ios --plist-file ios/RN061/Info.plist --sourcemap-output --output-dir ./build/ios/",
    "push-ios-history": "appcenter codepush deployment history -a zpd106/ruchen_ios Staging",
    "push-android": "appcenter codepush release-react -a zpd106/ruchen_android  --sourcemap-output --output-dir ./build/android/",
    "push-android-history": "appcenter codepush deployment history -a zpd106/ruchen_android Staging",
    "push-rollback-ios": "appcenter codepush rollback -a zpd106/ruchen_ios Staging",
    "push-rollback-android": "appcenter codepush rollback -a zpd106/ruchen_android Staging",
    "sentry-ios": "yarn sentry-cli react-native appcenter zpd106/ruchen_ios ios ./build/ios/CodePush ",
    "sentry-android": "yarn sentry-cli react-native appcenter zpd106/ruchen_android android ./build/android/CodePush",
    "clean": "cd android && ./gradlew clean && cd ..",
    "pod": "cd ios && pod install && cd ..",
    "adb": "adb reverse tcp:8081 tcp:8081",
    "log": "adb logcat | findstr com.rnshell.app",
    "test": "jest",
    "lint": "eslint .",
    "postinstall": "patch-package"
  },
