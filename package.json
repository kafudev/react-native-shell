  "scripts": {
    "start": "yarn react-native start",
    "android": "yarn react-native run-android",
    "ios": "yarn react-native run-ios --simulator 'iPhone Xr'",
    "ipad": "yarn react-native run-ios --simulator 'iPad Air 2'",
    "bundle-ios": "yarn react-native bundle --entry-file index.js --platform ios --dev false --bundle-output build/ios/bundle/index.jsbundle --sourcemap-output build/ios/bundle/index.jsbundle.map --assets-dest build/ios/bundle/",
    "build-ios": "yarn react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/index.jsbundle --sourcemap-output ios/index.jsbundle.map --assets-dest ios/",
    "bundle-android": "yarn react-native bundle --entry-file index.js --platform android --dev false --bundle-output build/android/bundle/index.android.bundle --sourcemap-output build/android/bundle/index.android.bundle.map --assets-dest build/android/bundle/",
    "build-android": "cd android && ./gradlew app:assembleRelease --profile && cd ../",
    "install-android": "cd android && ./gradlew app:installRelease --info --profile && cd ../",
    "push-ios": "appcenter codepush release-react -a kafudev/rnshell_ios --plist-file ios/rnshell/Info.plist --sourcemap-output --output-dir ./build/ios/",
    "push-ios-history": "appcenter codepush deployment history -a kafudev/rnshell_ios Staging",
    "push-android": "appcenter codepush release-react -a kafudev/rnshell_android  --sourcemap-output --output-dir ./build/android/",
    "push-android-history": "appcenter codepush deployment history -a kafudev/rnshell_android Staging",
    "push-rollback-ios": "appcenter codepush rollback -a kafudev/rnshell_ios Staging",
    "push-rollback-android": "appcenter codepush rollback -a kafudev/rnshell_android Staging",
    "sentry-ios": "yarn sentry-cli react-native appcenter kafudev/rnshell_ios ios ./build/ios/CodePush ",
    "sentry-android": "yarn sentry-cli react-native appcenter kafudev/rnshell_android android ./build/android/CodePush",
    "clean": "cd android && ./gradlew clean && cd ..",
    "pod": "cd ios && pod install && cd ..",
    "adb": "adb reverse tcp:8081 tcp:8081",
    "log": "adb logcat | findstr com.rnshell.app",
    "test": "jest",
    "lint": "eslint .",
    "release": "dotenv -- release-it prepatch --preRelease=beta --npm.tag=beta --github.preRelease",
    "release-ci": "dotenv -- release-it prepatch --preRelease=beta --npm.tag=beta --github.preRelease --ci",
    "postinstall": "patch-package"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "shell"
  ],
